    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ea2202c6-34bb-49ea-81af-31cfbdb1b6aa"
      }
    },
    "owner": "heylalit",
    "plugins": [
      "expo-secure-store"
    ]
